<swiper indicator-dots="true" autoplay="true" interval="3500" duration="1000" style='height:300rpx'>
  <block wx:for="{{banner_list}}" wx:key="{{item.id}}">
    <swiper-item>
      <image src="{{item.pic}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
<view class="list_item list_top_cover">
  <view class="list_top_item" wx:for="{{prefecture_list_top}}" wx:key="{{item.id}}">
    <image src="{{item.thumb}}" mode="widthFix"></image>
    <text>{{item.name}}</text>
  </view>
</view>
<view class="list_item">
  <block wx:if="{{count>0}}">
    <view wx:if="{{count==1}}">
      <image src="{{prefecture_list_middle[0].thumb}}" class="one-img img-radius" mode="widthFix"></image>
    </view>
    <view wx:if="{{count==2}}">
      <image src="{{item.thumb}}" wx:for="{{prefecture_list_middle}}" wx:key="{{index}}" class="two-img img-radius" mode="widthFix"></image>
    </view>
    <view wx:if="{{count==3}}">
      <view></view>
    </view>
    <view wx:if="{{count==4}}">
      <view class="four-cover">
        <image src="{{item.thumb}}" wx:for="{{prefecture_list_middle}}" wx:key="{{index}}" class="four-img img-radius" mode="widthFix"></image>
      </view>
    </view>
    <view wx:if="{{count==5}}"></view>
  </block>
</view>